{
  "name": "iono/stiletto",
  "description": "dependency injection container",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "container",
    "dependency injection"
  ],
  "authors": [
    {
      "name": "yuuki.takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/container": "~1.0",
    "container-interop/container-interop": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "satooshi/php-coveralls": "dev-master",
    "phpmd/phpmd": "*"
  },
  "autoload": {
    "psr-4": {
      "Iono\\Stiletto\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
